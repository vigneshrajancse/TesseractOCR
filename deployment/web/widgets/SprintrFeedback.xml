<?xml version="1.0" encoding="utf-8" ?>
<widget
  id="SprintrFeedbackWidget.SprintrFeedback"
  needsEntityContext="false"
  xmlns="http://www.mendix.com/widget/1.0/"
>
  <name>Feedback &amp; Collaboration Widget</name>
  <description>Widget to send feedback and collaborate with others on your Mendix Project</description>
  <icon></icon>
  <properties>
        <!--Authentication-->
        <property key="decryptedTokenEntity" type="entity" required="false" allowNonPersistableEntities="true">
            <caption>Decrypted Token Entity</caption>
            <category>MendixSSO Authentication</category>
            <description>The entity for the Decrypted Token of MendixSSO.</description>
        </property>
        <property key="decryptedTokenValue" type="attribute" entityProperty="decryptedTokenEntity" required="false">
            <caption>Decrypted Token Value</caption>
            <category>MendixSSO Authentication</category>
            <description>The Value attribute of Decrypted Token of MendixSSO</description>
            <attributeTypes>
                 <attributeType name="String" />
            </attributeTypes>
        </property>
        <property key="idTokenProviderMf" type="microflow" required="false">
            <caption>ID token provider</caption>
            <category>MendixSSO Authentication</category>
            <description>Microflow that will return the MendixSSO ID token. Required by App Switcher.</description>
            <returnType type="Object" entityProperty="decryptedTokenEntity" />
        </property>
        <property key="userProviderMf" type="microflow" required="false">
            <caption>User object provider</caption>
            <category>Custom Authentication</category>
            <description>Microflow that will return the User object for authentication if you do not use MendixSSO for authentication</description>
            <returnType type="Object" entityProperty="userEntity" />
        </property>
        <property key="userEntity" type="entity" required="false" allowNonPersistableEntities="true">
            <caption>User Object</caption>
            <category>Custom Authentication</category>
            <description>Entity that contains user name and email address attributes, that are used to prefill the user’s identity in the ‘Feedback’ submit form.</description>
        </property>
        <property key="usernameAttribute" type="attribute" entityProperty="userEntity" required="false">
            <caption>User name attribute</caption>
            <category>Custom Authentication</category>
            <description>Username attribute of User Object</description>
            <attributeTypes>
                 <attributeType name="String" />
            </attributeTypes>
        </property>
        <property key="emailAttribute" type="attribute" entityProperty="userEntity" required="false">
            <caption>Email attribute</caption>
            <category>Custom Authentication</category>
            <description>Email address attribute of User Object</description>
            <attributeTypes>
                 <attributeType name="String" />
            </attributeTypes>
        </property>
        <!-- PROJECT CONFIGURATION -->
        <property key="sprintrapp" type="string" required="true" defaultValue="1">
            <caption>App ID</caption>
            <category>Project</category>
            <description>The App ID is a hash that uniquely identifies your app project. You can find this ID in Settings > General in the Developer Portal.</description>
        </property>
        <property key="appName" type="string" required="true" defaultValue="">
            <caption>App name</caption>
            <category>Project</category>
            <description>The app project name used when sharing your app.</description>
        </property>
        <property key="allowScreenshot" type="boolean" defaultValue="true">
            <caption>Allow screenshots</caption>
            <category>Project</category>
            <description>If you are not allowed to send screenshots when submitting feedback (for legal reasons, etc.), set this to ‘No’.</description>
        </property>
        <property key="hideLogo" type="boolean" defaultValue="false">
            <caption>Hide Mendix logo</caption>
            <category>Project</category>
            <description>For customized branding, you can remove the Mendix logo from the widget.</description>
        </property>
        <!-- COLLABORATION -->
        <property key="allowEdit" type="boolean" defaultValue="true">
            <caption>Edit button</caption>
            <category>Collaboration</category>
            <description>Add the ‘Edit’ button to the widget menu for editing in the Web Modeler.</description>
        </property>
        <property key="allowMobileShare" type="boolean" defaultValue="true">
            <caption>Mobile view button</caption>
            <category>Collaboration</category>
            <description>Add the ‘Mobile view’ button to the widget menu for opening your application via the Mendix mobile app.</description>
        </property>
        <property key="allowNormalShare" type="boolean" defaultValue="true">
            <caption>Share button</caption>
            <category>Collaboration</category>
            <description>Add the ‘Share’ button to the widget menu for sharing your Mendix application.</description>
        </property>
        <!-- ADVANCED -->
        <property key="sprintrServer" type="string" required="true" defaultValue="https://sprintr.home.mendix.com/">
            <caption>Feedback server location</caption>
            <category>Advanced</category>
            <description>You can configure a new feedback server location if necessary, but Mendix recommends leaving this set to the default at https://sprintr.home.mendix.com/.</description>
        </property>
        <property key="screenshotForeignObjectRendering" type="boolean" defaultValue="true">
            <caption>Screenshot foreign rendering</caption>
            <category>Advanced</category>
            <description>This is enabled by default and an option in HTML2Canvas. Only disable this when you experience problems with creating screenshots.</description>
        </property>
        <property key="scrollableAreaSelector" type="string"  required="false" defaultValue=".mx-scrollcontainer-fixed > .mx-scrollcontainer-middle > .mx-scrollcontainer-wrapper">
            <caption>CSS selector for a scrollable area</caption>
            <category>Advanced</category>
            <description>If you use internal scrolling inside of a div container, screenshot feature might not work correctly. This allows you to specify this area with the CSS selector.</description>
        </property>
        <property key="hasCreateAppButton" type="boolean" defaultValue="true">
            <caption>Show Create New App button</caption>
            <category>Advanced</category>
            <description>App Switcher modal will provide a button that allows the user to create a new Mendix app.</description>
        </property>
        
